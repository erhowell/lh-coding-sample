{
  "extends": ["next/core-web-vitals", "next/typescript"],
  "rules": {
    "import/no-anonymous-default-export": [
      "error",
      {
        "allowObject": true
      }
    ],
    "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }],
    "no-undef": "error"
  },
  "globals": {
    "JSX": true
  }
}
